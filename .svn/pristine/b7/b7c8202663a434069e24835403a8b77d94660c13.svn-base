<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<script src="js/jquery-3.4.1.js"></script>
		<title></title>
	</head>
	<body>
		<input type="text" name="" id="doc" value="" style="width: 200px;height: 40px;background-color: #008B8B;"/>
		<button onclick="doc()">ok</button>
	</body>
	<script>
		
		function doc(){
			var doc= document.getElementById('doc');
			$.ajax({
				type:"POST",
				url:"http://localhost:80/robot/getDoc",
				data:{
					ul:doc.value
				},
				
				success:function(e){
					console.log(e)
				},
				error: function() {
					console.log(555)
				}
				
			})
		}
		
		
	</script>
</html>
