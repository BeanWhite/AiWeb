<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title></title>
		<script src="./config/urlConfig.js"></script>
	</head>
	<style type="text/css">
		#back {
			position: absolute;
			top: 0;
			bottom: 0;
			margin: auto 0;
			z-index: -10;
		}

		.menue {
			display: flex;
			justify-content: center;
			align-items: center;
			position: absolute;
			top: 0;
			bottom: 0;
			left: 0;
			right: 0;
			margin: auto;
			width: 100%;
			height: 100%;
			z-index: 10;
			/* background-color: rosybrown; */
		}

		#menue-test {
			padding: 0 20px 0 20px;
			margin-right: 15%;
			white-space: nowrap;
			text-align: center;
			line-height: 80px;
			height: auto;
			margin-left: 300px;
			background-color: rgba(255, 255, 255, 0.54);
			border-color: rgb(255, 255, 255);
			border-width: 1px;
			border-style: solid;
			color: rgb(255, 255, 255);
			border-radius: 5px;
			font-size: 28px;
			cursor: default;
		}

		#menue-report {
			padding: 0 20px 0 20px;
			white-space: nowrap;
			text-align: center;
			line-height: 80px;
			height: auto;
			margin-right: 300px;
			background-color: rgba(255, 255, 255, 0.54);
			border-color: rgb(255, 255, 255);
			border-width: 1px;
			border-style: solid;
			color: rgb(255, 255, 255);
			border-radius: 5px;
			font-size: 28px;
			cursor: default;
		}
		body {
			padding: 0;
			margin: 0;
			width: 100%;
			height: 100%;
		}
		
	</style>
	<body>

		<!-- 背景 -->
		<img id="back" src="../img/poster-about.jpg" style="width: 100%;height: 100%;">


		<!-- 菜单 -->
		<div class="menue">
			<div id="menue-test">
				开始评估
			</div>
			<div id="menue-report">
				历史报告
			</div>
		</div>
	</body>
	<script>
		let USER_ID = window.location.search.split('?')[1].split(0,1);
		document.getElementById('menue-test').onclick = function() {
			alert(1)
		};
		
		document.getElementById('menue-report').onclick = function() {
			alert(2)
		};
		
		var ws = new WebSocket(config.wsURL+'/websocket/'+USER_ID);
		ws.onopen = function(e){
			console.log('连接成功');
		}
		
		ws.onmessage = function(e){
			
			console.log('有消息推送',JSON.parse(e.data));
		}
		ws.onerror = function(e){
			console.log('连接出错');
		}
		ws.onclose = function(e){
			console.log('连接关闭');
		}
		function sendMSG(msg){
			ws.send(JSON.stringify(msg));
		}
		window.onbeforeunload = function(){
			ws.close();
		}
	</script>
</html>
