					<thead>
						<tr>
							<th v-for="(it,index) in mmse.title">
								<p>{{it}}</p>
							</th>
						</tr>
					</thead>
					<tr v-for="(it,index) in mmse.item" :style="{height:(it.img?'200px':'')}">
						<td>
							<p>{{it.title}}</p>
						</td>
						<td>
							<ul class="ul3">
								<li v-for="(it2,index2) in it.value">
									<p>{{it2}}</p>
								</li>
							</ul>
						</td>
						<td>
							<ul class="ul3">
								<li v-for="(it2,index2) in it.answer" style="position: relative;" :style="{height:(it.img?'200px':'')}">
									<img v-if="it.img" class="pr" :src="imgURL+'/'+'mmse'+msgObject[0].msg_b+'.png'" alt="" style="width: 100%;position: absolute;top: 0;bottom: 0;left: 0;right: 0;margin: auto;">
									<img src="'D://mmse222.png'" />
									<p v-else>{{it2}}</p>
								</li>
							</ul>
						</td>
						<td>
							<ul class="ul3">
								<li v-for="(it2,index2) in it.score">
									<p style="text-align:center">{{it2}}</p>
								</li>
							</ul>
						</td>
						<td>
							<p style="text-align: center;">{{it.result}}</p>
						</td>
					</tr>

					<tr>
						<td>
							<p>{{mmse.result.title}}</p>
						</td>
						<td colspan="4">
							<p>{{mmse.result.value}}</p>
						</td>
					</tr>
					<tr>
						<td>
							<p>{{mmse.conclusion.title}}</p>
						</td>
						<td colspan="4">
							<p>{{mmse.conclusion.value}}</p>
						</td>
					</tr>
				</table>
