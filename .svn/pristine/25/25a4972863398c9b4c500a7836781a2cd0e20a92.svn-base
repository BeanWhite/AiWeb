						<table class="table4">
							<thead>
								<tr>
									<td class="td" style="width:158.5px;">
										项目
									</td>
									<td class="td" style="width:120px;">
										得分
									</td>
									<td class="td" style="">
										结论参考值
									</td>
									<td class="td" style="width:158.5px;">
										结论
									</td>
								</tr>
							</thead>
							<tbody>
								<tr v-for="(yz,index1) in it.yz">
									<td>{{yz.title}}</td>
									<td>{{yz.score}}</td>
									<td v-if="yz.resultname!=null">{{yz.resultname}}:{{yz.start_fen}}--{{yz.end_fen}}</td>
									<td v-else>{{yz.start_fen}}--{{yz.end_fen}}</td>
									<td>{{yz.resultname}}</td>
								</tr>
							</tbody>
						</table>
						<p v-if="it.yz[0]['tzms']!=null" style="text-align: left;;font-size: 16px;font-weight: bold;height: 20px;text-decoration:underline;margin-left: 2px; ">
